{
  "expo": {
    "name": "Citizen Jaivik",
    "slug": "citizen-jaivik",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "citizenjaivik",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Fresh organic food delivery app for Jamshedpur",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.citizenjaivik.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#f8fdf8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.citizenjaivik.app",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f8fdf8"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcda0ef5-a6ab-45e3-9fbb-661afcf658db"
      }
    }
  }
}